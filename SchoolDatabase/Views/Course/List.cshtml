@model IEnumerable<SchoolDatabase.Models.Course>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page should show information about all courses</p>

@foreach(SchoolDatabase.Models.Course course in Model){

    <div><a class="lists" href="/Course/Show/@course.CourseId">@course.CourseId - @course.CourseName</a></div>


}